
<link rel="stylesheet" type="text/css" href="../Content/DefaultWithTable.css">

@model List<String>

<div class="leftSide">
    <div class="above">
        <div class="User leftSide">
            <img id="per" src="../Content/Person.png" alt="Person" />
            <div class="labels">
                <label>Username</label><br />
                <label>Teacher</label>
            </div>
        </div>
    </div>
    <div id="sidebar">
        <ul>
            <li id="sidebar-title">Courses</li>
            @foreach(var item in Model)
            {
                <li>@Html.DisplayFor(x => item)</li>
            }
        </ul>
    </div>
</div>