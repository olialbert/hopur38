@{
    ViewBag.Title = "User Page";
}

@using (Html.BeginForm())
{
    @model List<string>
    foreach (var item in Model)
    {
        <input type="hidden" value=@Html.DisplayFor(m => item) name="SearchName" />
        <input type="text" value=@Html.DisplayFor(m => item) name="Name" />
    }

    <input type="submit" value="submit" onclick="location.href='@Url.Action("Courses", "Admin" )'" />
}

<input type="button" value="Cancel" onclick="location.href='@Url.Action("Courses", "Admin")'" />