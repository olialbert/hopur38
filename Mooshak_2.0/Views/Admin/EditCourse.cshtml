@{
    ViewBag.Title = "User Page";
}

@using (Html.BeginForm())
{
    @model List<string>
    foreach (var element in Model)
    {
        
        <input type="hidden" value=@Html.DisplayFor(m => element) name="SearchName" />
        <input type="text" value=@Html.DisplayFor(m => element) name="Name" />
    }

    <input type="submit" value="submit" onclick="location.href='@Url.Action("Index", "Home" )'" />
}

<input type="button" value="Cancel" onclick="location.href='@Url.Action("Index", "Home")'" />