@{
    ViewBag.Title = "Admin";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model List<string>

@Html.Partial("_AdminForm")

<div class="main">
    <table class="course">
        <caption>Courses</caption>
        <thead>
            <tr>
                <th>Course</th>
                <th>Options</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var element in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(m => element)</td>
                    <td>
                        <a class="delete">Delete</a>
                        <a class="edit">Edit</a>
                        <a>View users</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>