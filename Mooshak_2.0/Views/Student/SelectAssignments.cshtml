@{
    ViewBag.Title = "Select Assignment";
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    @foreach (var element in Model.Assignments)
    {
        string name = element;
        <p> @Html.ActionLink(name, "SelecSubtAssignment", new { id = element + "," + Model.CourseName })</p>
    }
</body>
</html>
